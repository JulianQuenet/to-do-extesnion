{
    "manifest_version": 3,
    "name": "To-do",
    "version": "1.0.0",
    "description": "A handy to-do extension to keep track of all your tasks",
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "action": {
      "default_popup": "index.html",
      "default_title": "To-do"
    },
    "permissions": [
      "storage"
    ]
  }
  